<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox prefHeight="170.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="25.0" prefWidth="100.0" text="First Name" />
            <Label prefHeight="25.0" prefWidth="100.0" text="Last Name" />
            <Label prefHeight="25.0" prefWidth="100.0" text="Date of Birth" />
            <Label prefHeight="25.0" prefWidth="100.0" text="Gender" />
            <Label prefHeight="25.0" prefWidth="100.0" text="Website" />
         </children>
      </VBox>
   </left>
   <center>
      <VBox prefHeight="170.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textFirst" />
            <TextField fx:id="textLast" />
            <TextField fx:id="textDate" promptText="YYYY-MM-DD" />
            <TextField fx:id="textGender" />
            <TextField fx:id="textWebsite" />
            <Button mnemonicParsing="false" onMouseClicked="#onSaveClick" prefHeight="25.0" prefWidth="109.0" text="Save" />
            <Button mnemonicParsing="false" onMouseClicked="#onDeleteClick" prefHeight="0.0" prefWidth="110.0" text="Delete" />
         </children>
      </VBox>
   </center>
</BorderPane>
